{
  "name": "leo-translator",
  "displayName": "LeoTranslator",
  "description": "",
  "version": "0.0.2",
  "publisher": "LeoYoung",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.translateAndInsert",
    "onCommand:extension.translateAndReplace"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.translateAndInsert",
        "title": "Translate And Insert"
      },
      {
        "command": "extension.translateAndReplace",
        "title": "Translate And Replace"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@types/request": "0.0.40",
    "request": "^2.79.0"
  }
}
