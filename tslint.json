{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [
        true,
        "single"
      ],
      "no-string-literal": false,
      "trailing-comma": false,
      "arrow-parens": false,
      "indent": [
        true,
        "spaces"
      ],
      "align": true
    },
    "rulesDirectory": []
}
